			"id": "step_0-simple_bigquery_tool",
			"label": "Simple Big Query Tool",
			"description": "A simple tool to query BigQuery",
			"category": "Google",
			"formSource": "simple_bigquery",
			"version": "0.0",
			"author": "demo",
			"icon": {
				"fontawesome": ""
			},
			"params": {
				"direction": "ascending",
				"projectName": "appsoma-1",
				"dataSetName": "publicdata:samples",
				"table": "gsod",
				"columns": "max_temperature, year, month, day, station_number",
				"sortColumn": "max_temperature",
				"count": "10",
				"sortOrder": "Descending",
				"big_query_credentials": "$DATASETS/bigquery/bigquery-test.json"
			},
			"inputs": {
				"inputFile": {
					"id": "inputFile",
					"label": "Bigquery credential file",
					"type": "file",
					"dataType": "text"
				}
			},
			"outputs": {
				"outputFile": {
					"id": "outputFile",
					"label": "Query Results",
					"type": "file",
					"dataType": "text"
				}
			},
			"task": {
				"command": "python ./code/big_query/simple_big_query.py ./inputs/inputFile ./outputs/outputFile",
				"container_image": "appsoma/python-google-api",
				"requirements": {
					"cpus": 1,
					"mem": 512,
					"disk": 1024
				}
			},
			"x": 158,
			"y": 111,
			"tool": "simple_bigquery_tool"
		},
		{
			"id": "step_1-file_head",
			"label": "File Head",
			"version": "0.0",
			"author": "zack",
			"icon": {
				"fontawesome": ""
			},
			"category": "File Line Tools",
			"formSource": "file_head",
			"formCopy": {
				"version": "0.0",
				"components": [
					{
						"componentName": "filepicker",
						"context": {
							"label": "Input file",
							"name": "inputFile",
							"required": true
						}
					},
					{
						"componentName": "input",
						"context": {
							"label": "Number of lines",
							"name": "numberOfLines",
							"placeholder": "10",
							"help": "Must be a number",
							"validFunction": "",
							"required": true
						}
					}
				]
			},
			"params": {
				"numberOfLines": 10
			},
			"inputs": {
				"inputFile": {
					"id": "inputFile",
					"label": "Text file",
					"type": "file",
					"dataType": "text"
				}
			},
			"outputs": {
				"outputFile": {
					"id": "outputFile",
					"label": "Sorted file",
					"type": "file",
					"dataType": "text"
				}
			},
			"task": {
				"command": "head --lines=$PARAM_numberOfLines ./inputs/inputFile > ./outputs/outputFile",
				"container_image": "ubuntu:14.04",
				"requirements": {
					"cpus": 1,
					"mem": 512,
					"disk": 1024
				}
			},
			"x": 427,
			"y": 145,
			"tool": "file_head"
